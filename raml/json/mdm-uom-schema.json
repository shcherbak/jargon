{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Unit of Measure",
  "description": "Unit of Measure Type definition",
  "type": "object",
  "properties": {
    "uom_code": {
      "type": "string",
      "description": "ISO code of uom"
    },
    "uom_domain": {
      "type": "string"
    },
    "base_uom_code": {
      "type": "string"
    },
    "factor": {
      "default": 0,
      "description": "An explanation about the purpose of this instance.",
      "title": "The Quantity Schema",
      "type": "number",
      "minimum": 0,
      "maximum": 9999999999999999.9999,
      "multipleOf": 0.0001
    }
  },
  "additionalPoperties": false,
  "required": [
    "uom_code",
    "uom_domain",
    "base_uom_code",
    "factor"
  ]
}
